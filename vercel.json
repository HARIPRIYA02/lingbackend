{
"routes": [
  {
    "src": "/static/(.*)",           
    "dest": "/static/$1"
  },
  {
    "src": "/assets/(.*)",           
    "dest": "/assets/$1"
  },
  {
    "src": "/(.*\\.js)",
    "dest": "/$1"
  },
  {
    "src": "/(.*\\.css)",
    "dest": "/$1"
  },
  {
    "src": "/(.*\\.ico)",
    "dest": "/$1"
  },
  {
    "src": "/(.*\\.png)",
    "dest": "/$1"
  },
  
  {
    "src": "/questions",
    "dest": "/index.html"
  },
  {
    "src": "/leaderboard",
    "dest": "/index.html"
  },
  {
    "src": "/quiz",
    "dest": "/index.html"
  },
  {
    "src": "/",
    "dest": "/index.html"
  },
  {
    "src": "/.*",
    "dest": "/index.html"
  }
]
}